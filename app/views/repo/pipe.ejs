<h2>
  Please wait, we're downloading repository for you.
</h2>

<div id="output"></div>

<style>
  #output {
    padding-top: 40px;
    width: 400px;
    margin: 0 auto;
  }
  .commit {
    width: 20px;
    height: 20px;
    background: #999;
    margin: 3px;
    float: left;
  }
</style>

<hr/>

<script>
  var $output = $('#output');

  function pipe(type) {
    if (type === 'commit') {
      $output.append('<div class="commit"></div>');
    } else {
      $output.append('<div class="' + type + '"></div>');
    }
  }
</script>
